            <!-- MODAL -->
            <dialog class="dialog_modal" id="modal_horario">
                <div class="container_modal">
                    <section class="modal_titulo modal_nav">
                        <span class="modal_titulo">Agregando materia al horario</span>
                        <button class="btn_cerrar_modal" onclick="cerrarModalHorario()" id="cerrar_moda_horario"><i class="bi bi-x-lg"></i></button>
                    </section>
                    <section class="modal_body">
                        <form action="" method="dialog" class="formulario_modal">
                            <div class="container_inputs_materia_horario">
                                <label for="buscador_input">Materia:</label>
                                <input
                                    required
                                    class="input_modal"  
                                    list="buscador_materias" 
                                    name="buscador_input"
                                    id="buscador_input"
                                    title="Escriba y seleccione la materia que desea agregar al horario">
                                <datalist id="buscador_materias">
                                    <option value="fisica"></option>
                                </datalist>
                            </div>
                            <div class="container_input_dia_horario">
                                <label for="dia_horario_input">Dia:</label>
                                <input
                                    disabled
                                    required
                                    class="input_modal input_bloqueado" 
                                    list="lista_dias"  
                                    type="datetime" 
                                    id="dia_horario_input" 
                                    name="dia_horario_input"
                                    placeholder="Dia de clase"
                                    title="Seleccione el dia en el que va ver la materia">
                                <datalist id="lista_dias">
                                    <option value="Lunes"></option>
                                    <option value="Martes"></option>
                                    <option value="Miercoles"></option>
                                    <option value="Jueves"></option>
                                    <option value="Viernes"></option>
                                    <option value="Sabado"></option>
                                    <option value="Domingo"></option>
                                </datalist>
                            </div>
                            <div class="container_input_hora_horario"> 
                                <label for="hora_horario_input">Hora:</label>
                                <input
