                    <input 
                        title="Nombre de usuario" 
                        required 
                        data-input-login-name  
                        type="text" 
                        name="name" 
                        id="login_name" 
                        class="input_modal"
                    >
                </div>
                <div class="container">
                    <input 
                        title="ContraseÃ±a de usuario" 
                        required 
                        data-input-login-password  
                        type="password" 
                        name="password" 
                        id="login_password" 
                        class="input_modal"
                    >
                </div>
                <input data-form-login-btn type="submit" value="Enviar" class="input_submit">
            </form>
        </main>
    </body>
    <script src="../lib/Sweetalert2/sweetalert2.js"></script>  
    <script type="text/javascript" src="../components/login.js"></script>
